{
  "type": "record",
  "namespace": "example.avro.ably",
  "name": "Event",
  "doc": "This is the schema for an event, containts it time, capacity and booking window",
  "fields": [
       {
        "name": "eventId",
        "type": "string",
        "default": "no name"
      },
      {
        "name": "eventCapacity",
        "type": "int",
        "default": 100
      },
      {
          "name": "eventDate",
          "type": "long"
      },
      {
          "name": "bookingStart",
          "type": "long"
      },
      {
          "name": "bookingEnd",
          "type": "long"
      },
      {
        "name": "timestamp",
        "type": "long"
      }
    ]
  }